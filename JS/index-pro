let projectsHTML = '';

projects.forEach((projects) => {

  projectsHTML += `
  <section class="project-container">
    <section class="project-image">
      <img src="${projects.image}">

      <section class="project-year">
          <span>${projects.year}</span>
      </section>
    </section>

    <section class="project-name-tag">
        <seection>
            <span class="project-name">${projects.name}</span>
        </seection>
        <section class="project-tags">
            <span class="project-sector">${projects.sector}</span>
            <span class="project-category">${projects.project}</span>
        </section>
    </section>             
  </section>
  `
});


document.querySelector('.js-projects-container').innerHTML = projectsHTML; 






